<div id="sign-in-form">
  <h3>Sign In</h3>
  <%= form_for @changeset, session_path(@conn, :create), fn f -> %>
  <%= label(f, :email) %>
  <%= text_input(f, :email) %>

  <%= label(f, :password) %>
  <%= password_input(f, :password) %>

  <%= submit("Submit") %>
  <% end %>
  <%= link "Create Account", to: user_path(@conn, :new) %>
</div>
